<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration >
    <context id="mysql" targetRuntime="MyBatis3Simple">
        <commentGenerator>
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="org.mariadb.jdbc.Driver" connectionURL="jdbc:mariadb://localhost/logisticsDB"
                        userId="root" password="123456">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.entity" targetProject="e:/wuliu/src/main/java">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mappers" targetProject="E:/wuliu/src/main/resources">

        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.dao" targetProject="e:/wuliu/src/main/java">

        </javaClientGenerator>

        <table tableName="%">
            <generatedKey column="id" sqlStatement="Mysql"></generatedKey>
        </table>

    </context>

</generatorConfiguration>
